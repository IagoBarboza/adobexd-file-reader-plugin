{
  "manifestVersion": 4,
  "id": "123456",
  "name": "AdobeXD File Reader Plugin",
  "version": "0.0.1",
  "host": {
    "app": "XD",
    "minVersion": "36.0"
  },
  "entryPoints": [
    {
      "type": "command",
      "id": "insertTextFromFile",
      "label": {
        "default": "Insert Text From File"
      },
      "shortcut": {
        "mac": "Cmd+Alt+I",
        "windows": "CtrlAlt+I"
      }
    }
  ],
  "icons": [
    {
      "width": 24,
      "height": 24,
      "path": "icons/icon.png",
      "scale": [1,2],
      "theme": ["all"],
      "species": ["pluginList"]
    }
  ]
}